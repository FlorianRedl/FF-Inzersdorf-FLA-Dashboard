<Page x:Class="Zeitmessung.AnalysePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Zeitmessung"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AnalysePage">

    <Grid>
        <Border  Grid.Row="2" Grid.Column="1" Background="#F2EFED" CornerRadius="3">
            <StackPanel  Margin="5" Orientation="Horizontal" FlowDirection="RightToLeft" VerticalAlignment="Center" HorizontalAlignment="Center">
                <StackPanel>
                    <TextBlock Text="Analyse" FontSize="24"/>
                </StackPanel>
            </StackPanel>
        </Border>
    </Grid>
</Page>
